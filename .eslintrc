{
    "env": {
        "browser": false,
        "commonjs": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": ["warn", { "varsIgnorePattern": "^_" }]
    }
}
